generator client {
  provider = "prisma-client-js"
  previewFeatures = ["transactionApi"]
  binaryTargets = ["darwin", "windows", "native", "linux-musl", "debian-openssl-1.0.x", "debian-openssl-1.1.x"]
  output = "../generated/postgres-migrator"
}

datasource postgres {
  provider = "postgres"
  url      = env("POSTGRES_URL")
}
